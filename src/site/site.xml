<project name="TMD-Maker" xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd">

	<bannerLeft>
		<name>TMD-Maker</name>
		<src>img/tmd_icon_48_32bit.png</src>
		<href>https://tmdmaker.osdn.jp</href>
	</bannerLeft>
	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="bottom" />
	<googleAnalyticsAccountId>UA-96558945-1</googleAnalyticsAccountId>

	<poweredBy>
		<logo name="Eclipse Graphical Editing Framework" href="https://www.eclipse.org/gef/" img="https://wiki.eclipse.org/images/thumb/2/2d/GEF-logo.png/180px-GEF-logo.png" ></logo>
		<logo name="OSDN" href="https://osdn.net/" img="//osdn.net/sflogo.php?group_id=4743&amp;type=2" alt="OSDN" width="125" height="38" border="0"></logo>
	</poweredBy>

	<skin>
		<groupId>com.bernardomg.maven.skins</groupId>
		<artifactId>docs-maven-skin</artifactId>
		<version>2.0.10</version>
	</skin>

	<body>
		<head>
		<![CDATA[
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<link rel="stylesheet" href="./css/tmd.css" />
		]]>
		</head>
		<links>
			<item name="Download" href="https://osdn.net/projects/tmdmaker/releases/" />
		</links>

		<menu name="Document" inherit="top">
			<item name="Release Version" >
				<item name="Manual" href="./doc/index.html" />
				<item name="FAQ" href="./doc/faq.html" />
			</item>
			<item name="Latest" href="https://tmdmaker.readthedocs.io/ja/latest/index.html" />
	    </menu>

		<menu name="Lang" inherit="top">
			<item name="en" href="./" />
			<item name="ja" href="./ja/" />
	    </menu>

		<menu name="Project" inherit="top">
			<item name="GitHub" href="https://github.com/tmdmaker/tmdmaker/" />
		</menu>

	    <menu name="Project Document" ref="reports" inherit="bottom" />

	</body>

	<custom>
		<skinConfig>
			<twitterSite>@TMDMakerTweet</twitterSite>
			<topNav>
            <menu>Document</menu>
            <menu>Project</menu>
            <menu>Project Document</menu>
            <menu>Lang</menu>
			</topNav>
		</skinConfig>
	</custom>
</project>
