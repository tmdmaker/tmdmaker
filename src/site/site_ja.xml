<project name="TMD-Maker" xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd">

	<bannerLeft>
		<name>TMD-Maker</name>
		<src>img/tmd_icon_48_32bit.png</src>
		<href>https://tmdmaker.osdn.jp</href>
	</bannerLeft>
	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="bottom" />
	<googleAnalyticsAccountId>UA-96558945-1</googleAnalyticsAccountId>

	<poweredBy>
		<logo name="Eclipse Graphical Editing Framework" href="https://www.eclipse.org/gef/" img="https://wiki.eclipse.org/images/thumb/2/2d/GEF-logo.png/180px-GEF-logo.png" ></logo>
		<logo name="OSDN" href="https://osdn.net/" img="//osdn.net/sflogo.php?group_id=4743&amp;type=2" alt="OSDN" width="125" height="38" border="0"></logo>
	</poweredBy>

	<skin>
		<groupId>com.bernardomg.maven.skins</groupId>
		<artifactId>docs-maven-skin</artifactId>
		<version>2.0.10</version>
	</skin>

	<body>
		<head>
		<![CDATA[
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<link rel="stylesheet" href="./css/tmd.css" />
		]]>
		</head>
		<links>
			<item name="ダウンロード" href="https://osdn.net/projects/tmdmaker/releases/" />
		</links>

		<menu name="ドキュメント" inherit="top">
			<item name="リリースバージョン" >
				<item name="マニュアル" href="./doc/index.html" />
				<item name="よくある質問" href="./doc/faq.html" />
			</item>
			<item name="Latest" href="https://tmdmaker.readthedocs.io/ja/latest/index.html" />
	    </menu>

		<menu name="プロジェクト" inherit="top">
			<item name="GitHub" href="https://github.com/tmdmaker/tmdmaker/" />
		</menu>

	    <menu name="プロジェクト文書" ref="reports" inherit="bottom" />

	</body>

	<custom>
		<skinConfig>
			<twitterSite>@TMDMakerTweet</twitterSite>
		</skinConfig>
		<reflowSkin>
			<absoluteResourceURL>https://www.tmdmaker.org</absoluteResourceURL>
			<protocolRelativeURLs>true</protocolRelativeURLs>
			<smoothScroll>true</smoothScroll>
			<highlightJs>true</highlightJs>	
			<brand>
				<name>TMD-Maker</name>
				<href>https://www.tmdmaker.org</href>
			</brand>
			<slogan> <![CDATA[
			<p>ビジネス解析とデータベース設計を同時に実現する</p>
			<p />
			<a href='https://travis-ci.org/tmdmaker/tmdmaker'><img src='https://travis-ci.org/tmdmaker/tmdmaker.svg?branch=develop' alt='Build Status' /></a>
			<a href='https://coveralls.io/github/tmdmaker/tmdmaker?branch=develop'><img src='https://coveralls.io/repos/github/tmdmaker/tmdmaker/badge.svg?branch=develop' alt='Coverage Status' /></a>
			]]>
			</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<toc>top</toc>
			<navbar filterMenu="ドキュメント|プロジェクト|プロジェクト文書"></navbar>
			<bottomNav>
        		<column>ドキュメント</column>
        		<column>プロジェクト文書</column>
			</bottomNav>
			<breadcrumbs>false</breadcrumbs>
			<pages>
        		<index>
        			<toc>false</toc>
					<markPageHeader>false</markPageHeader>
					<sections>
						<carousel />
						<columns>2</columns>
						<body />
						<sidebar />
					</sections>
				</index>
			</pages>
		</reflowSkin>
	</custom>
</project>
