<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.3"?>
<plugin>
   <extension-point id="tmdmaker.generators" name="generators" schema="schema/tmdmaker.generators.exsd"/>
   <extension-point id="tmdmaker.dialect.provider" name="Dialect Provider" schema="schema/tmdmaker.dialect.provider.exsd"/>
   <extension-point id="tmdmaker.persisitent.serializer" name="tmdmaker.persisitent.serializer" schema="schema/tmdmaker.persisitent.serializer.exsd"/>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="jp.sourceforge.tmdmaker.TMDEditor"
            contributorClass="jp.sourceforge.tmdmaker.TMDEditorContributor"
            default="true"
            extensions="tmd"
            icon="icons/entity.gif"
            id="tmdmaker.tmdeditor"
            name="%editor.name.0">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="tmdmaker.category"
            name="%category.name.0">
      </category>
      <wizard
            category="tmdmaker.category"
            class="jp.sourceforge.tmdmaker.wizard.NewDiagramWizard"
            icon="icons/entity.gif"
            id="tmdmaker.wizard.newdiagramwizard"
            name="%wizard.name.0">
      </wizard>
   </extension>
   <extension
         point="tmdmaker.persisitent.serializer">
      <serializer
            class="jp.sourceforge.tmdmaker.persistent.impl.XStreamSerializer"
            name="Default Serializer">
      </serializer>
   </extension>

</plugin>
